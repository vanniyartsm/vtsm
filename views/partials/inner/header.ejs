    <!-- ===============================================-->
    <!--    Favicons-->
    <!-- ===============================================-->
    <link rel="apple-touch-icon" sizes="180x180" href="<%= locals.context %>/asm/assets/img/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="<%= locals.context %>/asm/assets/img/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="<%= locals.context %>/asm/assets/img/favicons/favicon-16x16.png">
    <link rel="shortcut icon" type="image/x-icon" href="<%= locals.context %>/asm/assets/img/favicons/favicon.ico">
    <link rel="manifest" href="<%= locals.context %>/asm/assets/img/favicons/manifest.json">
    <meta name="msapplication-TileImage" content="<%= locals.context %>/asm/assets/img/favicons/mstile-150x150.png">
    <meta name="theme-color" content="#ffffff">
    <meta name="csrf-token" content="{{<%= locals.csrfToken %>}}">
    <script src="<%= locals.context %>/asm/vendors/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="<%= locals.context %>/asm/vendors/simplebar/simplebar.min.js"></script>
    <script src="<%= locals.context %>/asm/assets/js/config.js"></script>


    <!-- ===============================================-->
    <!--    Stylesheets-->
    <!-- ===============================================-->
    <link href="<%= locals.context %>/asm/vendors/flatpickr/flatpickr.min.css" rel="stylesheet">
    <link href="<%= locals.context %>/asm/vendors/prism/prism-okaidia.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="">
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:wght@300;400;600;700;800;900&amp;display=swap" rel="stylesheet">
    <link href="<%= locals.context %>/asm/vendors/simplebar/simplebar.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.8/css/line.css">
    <link href="<%= locals.context %>/asm/assets/css/theme-rtl.min.css" type="text/css" rel="stylesheet" id="style-rtl">
    <link href="<%= locals.context %>/asm/assets/css/theme.min.css" type="text/css" rel="stylesheet" id="style-default">
    <link href="<%= locals.context %>/asm/assets/css/user-rtl.min.css" type="text/css" rel="stylesheet" id="user-style-rtl">
    <link href="<%= locals.context %>/asm/assets/css/user.min.css" type="text/css" rel="stylesheet" id="user-style-default">
	<link href="<%= locals.context %>/asm/assets/css/asm.css" type="text/css" rel="stylesheet" id="user-style-default">
	<script src="https://cdn.jsdelivr.net/npm/gasparesganga-jquery-loading-overlay@2.1.6/dist/loadingoverlay.min.js"></script>
    <script>
      var phoenixIsRTL = window.config.config.phoenixIsRTL;
      if (phoenixIsRTL) {
        var linkDefault = document.getElementById('style-default');
        var userLinkDefault = document.getElementById('user-style-default');
        linkDefault.setAttribute('disabled', true);
        userLinkDefault.setAttribute('disabled', true);
        document.querySelector('html').setAttribute('dir', 'rtl');
      } else {
        var linkRTL = document.getElementById('style-rtl');
        var userLinkRTL = document.getElementById('user-style-rtl');
        linkRTL.setAttribute('disabled', true);
        userLinkRTL.setAttribute('disabled', true);
      }
    </script>